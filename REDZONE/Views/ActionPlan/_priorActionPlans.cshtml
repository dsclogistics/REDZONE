@model IEnumerable<REDZONE.Models.PriorActionPlan>

<br />
<div class="bordered" style="border-color:darkgray; background-color:darkgray"><h4 style="font-weight:bold; color:white">Prior Month Action Plans</h4></div>
            //foreach{
<br />
<div class="btn-toolbar">
    <div class="bordered" style="display:table-cell; border-top-left-radius:4px; border-bottom-left-radius:4px; border-color:darkgray; border-right:transparent">
        <button type="button" class="btn btn-default btn-group" style="margin-left:0px; border-color:transparent; border-top-right-radius:0px; border-bottom-right-radius:0px">
            <span class="glyphicon glyphicon-chevron-right"></span>
        </button>
    </div>
    <div class="bordered" style="display:table-cell; width:100%; border-top-right-radius:4px; border-bottom-right-radius:4px; vertical-align:middle; border-color:darkgray;">
        <h4 class="btn-group" style="text-align:left; margin:auto; margin-left:5px ">//MONTH// Action Plan</h4>
    </div>
</div>

<div class="collapse" style="margin-left:40px">
    <h4 style="text-align:left; color:red; margin-top:10px">Action Plan Status: //STATUS//</h4>
    <div class="bordered" style="padding:15px; text-align:left; border-radius:4px; border-color:darkgray; min-height:100px">
        <div class="row">
            <div class="col-md-6" style="margin:0 auto; text-align:left; margin-bottom:8px; margin-top:5px">
                <!-- Metric Period Header Information  -->
                <div class="row" style="margin:0; font-weight:bold">
                    <div class="col-xs-2 headerCell" style="background-color:#f0eaea">Metric</div>
                    <div class="col-xs-2 headerCell" style="background-color:#bbedff">Goal</div>
                    <div class="col-xs-2 headerCell" style="background-color:#f0eaea"><span id="metricDate1">//MONTH//</span></div>
                </div>
                <div class="row" style="margin:0">
                    <div class="col-xs-2 headerCell" style="background-color:#f0eaea"><span class="mNameCell">//mNAME//</span></div>
                    <div class="col-xs-2 headerCell" style="background-color:#bbedff; color:blue; font-weight:bold;"><span id="mGoalCell">//mGOAL//</span></div>
                    <div class="col-xs-2 headerCell" id="mValueCell" style="font-weight:normal">//mVALUE//</div>
                </div>
            </div> <!-- END OF HEADER INFORMATION-->

            <div class="col-md-6">
                <ul style="text-align:left">
                    <li>Reason 1</li>
                    <li>Reason 2</li>
                </ul>
            </div>
        </div>
        <br />

        <div>Action Plan Text</div>
    </div>
    <div style="text-align:left;color:gray">Submitted By: //SUBMITTED BY//</div>
</div>
<br />


